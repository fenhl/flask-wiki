{% extends 'base.html.j2' %}

{% block title %}article not found — {{wiki_name}}{% endblock %}

{% block page_content %}
    <h1>Article not found</h1>
    <p>There is no article called “{{title}}” in the wiki namespace “{{namespace}}”.</p>
    <p>
        <a href="{{url_for('wiki_article_edit', title=title, namespace=namespace)}}">Create this article</a>
        • <a href="{{url_for('wiki_index')}}">{{wiki_name}} main page</a>
    </p>
{% endblock %}
